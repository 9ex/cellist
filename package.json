{
  "name": "cellist",
  "version": "0.0.1",
  "description": "cross-platform web traffic analyzer like fiddler",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "directories": {},
  "dependencies": {
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.0.0",
    "pre-commit": "^1.2.2",
    "proxy-agent": "^3.0.3",
    "superagent": "^4.1.0"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .js",
    "test-cov": "jest --coverage"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": "node-cellist/cellist",
  "keywords": [
    "proxy"
  ],
  "author": "rexzheng",
  "license": "MIT",
  "jest": {
    "testMatch": [
      "**/test/**/*.js",
      "!**/test/helper/*.js"
    ],
    "bail": true,
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/src/*.js"
    ],
    "coverageReporters": [
      "text-summary",
      "lcov"
    ]
  }
}