{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "ecmaFeatures": {
            "jsx": false
        },
        "sourceType": "module"
    },
    "plugins": [],
    "rules": {
        "no-cond-assign": [
            2,
            "except-parens"
        ],
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-semi": 1,
        "no-func-assign": 2,
        "no-invalid-regexp": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "no-unsafe-finally": 2,
        "no-unsafe-negation": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "no-redeclare": 2,
        "no-delete-var": 2,
        "no-undef": 2,
        "no-unused-vars": 1,
        "no-const-assign": 2,
        "no-template-curly-in-string": 2
    }
}